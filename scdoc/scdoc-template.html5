<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head>
  <meta charset="utf-8">
  <meta name="generator" content="pandoc, scdoc">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(tags)$
  <meta name="keywords" content="$tags$">
$endif$
$if(prefix-fig)$
  <meta name="prefix-fig" content="$prefix-fig$">
$endif$
$if(prefix-table)$
  <meta name="prefix-table" content="$prefix-table$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <!-- Waiting new realease of pandoc including https://github.com/jgm/pandoc/issues/1080
  <script type="text/javascript"
    src="http://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML">
  </script>
  -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<div class="title">$title$</div>
$if(subtitle)$
<div class="subtitle">$subtitle$</div>
$endif$
$for(author)$
<div class="author">$author$</div>
$endfor$
$if(date)$
<div class="date">$date$</div>
$endif$
$if(abstract)$
<div class="abstract">$abstract$</div>
$endif$
$if(tags)$
<div class="tags"><i class="icon-tags"></i> $tags$</div>
$endif$
</header>
$endif$

<a class="trigger" href="#"><i class="icon-menu"></i>&nbsp;</a>
<nav id="toc">
$if(toc-title)$
  <h1 class="notoc">$toc-title$</h1>
$endif$
</nav>
<section>
$body$
</section>
<nav id="tof">
$if(tof-title)$
  <h1>$tof-title$</h1>
$endif$
</nav>
<nav id="tot">
$if(tot-title)$
  <h1>$tot-title$</h1>
$endif$
</nav>
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
